{
  "name": "notes-sdk",
  "description": "Tanstack queries and shared components for notes service",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./index.ts",
    "./fakes": "./fakes.ts",
    "./strings": "./strings.ts",
    "./components": "./components.ts"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "vue-tsc",
    "dev": "NODE_OPTIONS='--experimental-strip-types --disable-warning=ExperimentalWarning' DOMAIN=localhost:5173 vite",
    "dev-docker": "npm exec saf-docker generate && docker compose up --build"
  },
  "devDependencies": {
    "@saflib/vitest": "*",
    "@testing-library/vue": "^8.1.0"
  },
  "dependencies": {
    "@automerge/automerge": "^3.1.2",
    "@automerge/automerge-repo": "^2.3.1",
    "@automerge/automerge-repo-storage-indexeddb": "^2.3.1",
    "@saflib/monorepo": "*",
    "@saflib/sdk": "*",
    "@saflib/vite": "*",
    "@saflib/vue": "*",
    "web-notes-links": "*",
    "msw": "^2.11.3",
    "notes-spec": "*",
    "vite-plugin-wasm": "^3.5.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
